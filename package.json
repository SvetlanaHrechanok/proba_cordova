    "cordova-plugin-advanced-http": "^2.3.1",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "cors": "^2.8.5",
    "jquery": "^3.4.1",
    "jsforce": "^1.9.3",
    "leaflet": "^1.6.0"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "1"
